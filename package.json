{
  "name": "openlist-sdk",
  "description": "OpenList SDK.",
  "author": "amatsuka <amatsukamao@qq.com>",
  "version": "0.1.0",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
